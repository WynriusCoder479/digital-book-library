		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^10.0.3",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "^9.4.2",
		"@nestjs/throttler": "^4.0.0",
		"@nestjs/typeorm": "^9.0.1",
		"@types/morgan": "^1.9.4",
		"argon2": "^0.30.3",
		"cache-manager": "^5.2.2",

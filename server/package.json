		"nodemailer": "^6.9.3",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.1",
		"pg": "^8.11.0",
		"redis": "^4.6.7",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
